{
  "name": "@henry781/ngx-terminal-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "cd projects/ngx-terminal && npm run clean",
    "watch-lib": "cd projects/ngx-terminal && npm run watch",
    "build-lib": "cd projects/ngx-terminal && npm run build",
    "start": "npm run clean && run-p watch-lib start-app",
    "start-app": "wait-on projects/ngx-terminal/dist/lib/api.d.ts && npm link @henry781/ngx-terminal && ng serve",
    "prebuild": "npm run build-lib",
    "build": "npm link @henry781/ngx-terminal && ng build ngx-terminal-demo",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.3",
    "ansi_up": "^4.0.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@angular/language-service": "~11.1.0",
    "@types/jasmine": "~3.6.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.22",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.0.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.1.3",
    "wait-on": "^5.2.1"
  }
}
